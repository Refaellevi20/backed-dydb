{
  "name": "server",
  "version": "1.0.0",
  "description": "production ready server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "server:dev": "nodemon server.js",
    "server:prod": "set NODE_ENV=production&&node server.js",
    "server:prod:win": "set NODE_ENV=production&&node server.js",
    "build": "npm install",
    "deploy": "aws s3 sync ./public s3://your-bucket-name",
    "deploy:s3": "aws s3 sync ./public s3://your-bucket-name --delete",
    "deploy:full": "npm run build && npm run deploy:s3",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:docker": "node scripts/deploy-docker.js",
    "s3:create": "docker-compose run --rm aws-cli s3 mb s3://${S3_BUCKET_NAME} --region ${AWS_REGION}",
    "s3:configure": "docker-compose run --rm aws-cli s3 website s3://${S3_BUCKET_NAME} --index-document index.html",
    "s3:deploy": "node scripts/deploy-s3-docker.js",
    "s3:setup": "docker-compose up -d && npm run s3:deploy"
  },
  "author": "Me",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.782.0",
    "@aws-sdk/lib-dynamodb": "^3.782.0",
    "@aws-sdk/client-s3": "^3.485.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cryptr": "^6.3.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "mongodb-connection-string-url": "^3.0.1",
    "mongoose": "^8.9.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
